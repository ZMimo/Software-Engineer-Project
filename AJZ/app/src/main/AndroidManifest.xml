<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ajz">
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <uses-feature
        android:name="android.hardware.camera"
        android:required="false"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AJZ">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Camera"
            android:label="Camera"
            android:parentActivityName=".Admin_Activity">
        </activity>
        <activity
            android:name=".Admin_Activity"
            android:label="Menu">
        </activity>
        <activity
            android:name=".MasterData_Activity"
            android:label="Master Data"
            android:parentActivityName=".Admin_Activity">
        </activity>
        <activity
            android:name=".Profit_Activity"
            android:label="Profit Data"
            android:parentActivityName=".Admin_Activity">
        </activity>
    </application>

</manifest>